{
  "name": "@fjell/registry",
  "description": "Dependency injection and service location system for the Fjell ecosystem",
  "version": "4.4.21",
  "keywords": [
    "registry",
    "dependency-injection",
    "service-locator",
    "fjell"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts --fix",
    "dev": "concurrently \"tsc --noEmit --watch\" \"node build.js --watch\"",
    "build": "npm run lint && tsc --noEmit && node build.js",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "npm run lint && vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/core": "file:../fjell-core",
    "@fjell/logging": "file:../fjell-logging"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@fjell/eslint-config": "file:../fjell-eslint-config",
    "@swc/core": "^1.13.2",
    "@tsconfig/recommended": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "3.2.4",
    "concurrently": "^9.2.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "vitest": "3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/registry.git"
  }
}
